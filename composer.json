{
    "name": "golchha21/icons-as-components",
    "version": "1.0.0",
    "description": "A package for using the svg icons as laravel blade components.",
    "keywords": [
        "golchha21",
        "laravel",
        "laravel-package",
        "laravel-component",
        "icons",
        "bootstrap icons",
        "php"
    ],
    "homepage": "https://github.com/golchha21/icons-as-components",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ulhas Vardhan Golchha",
            "email": "vardhans@ulhas.net",
            "homepage": "https://www.ulhas.net",
            "role": "Web Architect"
        }
    ],
    "support": {
        "email": "vardhans@ulhas.net",
        "issues": "https://github.com/golchha21/icons-as-components/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "laravel/framework": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Golchha21\\IconsAsComponents\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Golchha21\\IconsAsComponents\\Providers\\ServiceProvider"
            ]
        }
    }
}
